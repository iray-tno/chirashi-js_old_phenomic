{
    "root": true,
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": true,
            "optionalDependencies": false
        }],
        no-underscore-dangle: ["error", {
            "allow": [
                "__COLLECTION__",
                "__INITIAL_STATE__"
            ]
        }]
    }
}
